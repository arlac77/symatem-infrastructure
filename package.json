{
  "name": "symatem-infrastructure",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/module.js",
  "module": "src/module.js",
  "description": "infrastructure taxometry with Symatem",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Alexander MeiÃŸner"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "sym-infra-util": "bin/sym-infra-util"
  },
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/module.js --section=API",
    "lint": "documentation lint src/module.js"
  },
  "dependencies": {
    "SymatemJS": "https://github.com/Symatem/SymatemJS.git",
    "caporal": "^1.0.0",
    "dns-zonefile": "^0.2.2",
    "plist": "^3.0.0",
    "symatem": "^1.16.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "ava": "^3.13.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.3.1",
    "documentation": "^13.0.2",
    "rollup": "^2.11.2",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.6.0",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=14.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/symatem-infrastructure.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/symatem-infrastructure/issues"
  },
  "homepage": "https://github.com/arlac77/symatem-infrastructure#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-node-app",
      "arlac77/template-github"
    ]
  }
}
