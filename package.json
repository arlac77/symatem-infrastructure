{
  "name": "symatem-infrastructure",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/module.js",
  "module": "src/module.js",
  "description": "infrastructure taxometry with Symatem",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    },
    {
      "name": "Alexander MeiÃŸner"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "sym-infra-util": "bin/sym-infra-util"
  },
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/module.js --section=API",
    "lint": "documentation lint src/module.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava"
  },
  "dependencies": {
    "SymatemJS": "https://github.com/Symatem/SymatemJS.git",
    "caporal": "^1.0.0",
    "dns-zonefile": "^0.2.2",
    "plist": "^3.0.0",
    "symatem": "^1.16.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "acorn-class-fields": "https://github.com/arlac77/acorn-class-fields.git",
    "ava": "^2.4.0",
    "builtin-modules": "^3.1.0",
    "c8": "^7.0.0",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "markdown-doctest": "^1.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-executable": "^1.5.2",
    "semantic-release": "^16.0.0"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/symatem-infrastructure.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/symatem-infrastructure/issues"
  },
  "homepage": "https://github.com/arlac77/symatem-infrastructure#readme",
  "ava": {
    "files": [
      "tests/*-test.mjs"
    ],
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
